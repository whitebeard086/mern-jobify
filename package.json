{
  "name": "mern-jobify",
  "version": "1.0.0",
  "description": "MERN app for tracking jobs",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "server": "nodemon server --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\""
